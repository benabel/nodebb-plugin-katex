{
  "name": "nodebb-plugin-katex",
  "version": "0.3.0",
  "description": "Use Katex math parser inside NodeBB",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/benjaminabel/nodebb-plugin-katex"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "katex",
    "latex",
    "tex"
  ],
  "author": "Benjamin Abel <bbig26@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjaminabel/nodebb-plugin-katex/issues"
  },
  "dependencies": {
    "katex": "^0.7.0"
  },
  "devDependencies": {
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0"
  },
  "scripts": {
    "lint": "eslint **/*.js",
    "fix": "eslint --fix **/*.js",
    "format": "clang-format -i --glob=lib/**/*.js && clang-format -i --glob=static/**/*.js",
    "test": "npm run lint"
  },
  "engines": {
    "node": ">=4"
  },
  "nbbpm": {
    "compatibility": "^1.0.0"
  }
}
