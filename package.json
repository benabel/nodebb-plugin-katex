{
  "name": "nodebb-plugin-katex",
  "version": "1.0.0-semantic-release",
  "description": "Use Katex math parser inside NodeBB",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/benjaminabel/nodebb-plugin-katex"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "katex",
    "latex",
    "tex"
  ],
  "author": "Benjamin Abel <bbig26@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjaminabel/nodebb-plugin-katex/issues"
  },
  "dependencies": {
    "katex": "^0.6.0"
  },
  "devDependencies": {
    "eslint": "^3.12.1",
    "eslint-config-google": "^0.7.0",
    "semantic-release": "^4.3.5"
  },
  "scripts": {
    "lint": "eslint **/*.js",
    "fix": "eslint --fix **/*.js",
    "format": "clang-format -i --glob=lib/**/*.js && clang-format -i --glob=static/**/*.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "npm run lint"
  },
  "nbbpm": {
    "compatibility": "^1.0.0"
  }
}
